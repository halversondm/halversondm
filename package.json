{
  "name": "halversondm",
  "version": "1.0.0",
  "description": "The personal website of Dan Halverson - @halversondm",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "prestart": "npm run clean && cp -R app/runtime/* dist && cd dist && npm install",
    "start": "cross-env NODE_ENV=development concurrently \"npm run webpack -- --watch\" \"node dist/server.js\"",
    "eslint": "eslint app/**/**.js --fix",
    "test": "wdio wdio.conf.js",
    "prewebpack": "npm run eslint",
    "webpack": "webpack --config ./webpack.config.js --profile --colors",
    "tsc": "tsc -w",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production npm run webpack"
  },
  "keywords": [
    "react"
  ],
  "author": "Dan Halverson <daniel.m.halverson@gmail.com> (http://halversondm.com/)",
  "license": "MIT",
  "dependencies": {
    "@types/react": "15.0.4",
    "@types/react-bootstrap": "0.0.43",
    "@types/react-dom": "0.14.21",
    "@types/react-router": "2.0.46",
    "aws-sdk": "2.4.13",
    "connect-history-api-fallback": "1.3.0",
    "react": "15.2.1",
    "react-bootstrap": "0.29.5",
    "react-dom": "15.2.1",
    "react-router": "2.5.1"
  },
  "devDependencies": {
    "assert": "1.4.1",
    "awesome-typescript-loader": "3.1.3",
    "body-parser": "1.15.2",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "1.0.8",
    "css-loader": "0.28.0",
    "eslint": "3.16.1",
    "eslint-config-google": "0.7.1",
    "eslint-plugin-react": "6.10.0",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "mocha": "3.2.0",
    "morgan": "1.7.0",
    "rimraf": "2.4.3",
    "style-loader": "0.16.1",
    "typescript": "2.1.5",
    "wdio-junit-reporter": "0.2.0",
    "wdio-mocha-framework": "0.5.7",
    "webdriverio": "4.5.0",
    "webpack": "2.4.1"
  }
}
