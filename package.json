{
  "name": "halversondm",
  "version": "1.0.0",
  "description": "The personal website of Dan Halverson - @halversondm",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "prestart": "npm run clean",
    "start": "concurrently \"npm run webpack\" \"node server.js\"",
    "prebuild": "npm run clean && npm run eslint",
    "eslint": "eslint app/**/**.js --fix",
    "test": "wdio wdio.conf.js",
    "webpack": "npm run build -- --watch",
    "tsc": "tsc -w",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --profile --colors"
  },
  "keywords": [
    "react"
  ],
  "author": "Dan Halverson <daniel.m.halverson@gmail.com> (http://halversondm.com/)",
  "license": "MIT",
  "dependencies": {
    "@types/react": "15.0.4",
    "@types/react-bootstrap": "0.0.43",
    "@types/react-dom": "0.14.21",
    "@types/react-router": "2.0.46",
    "aws-sdk": "^2.4.13",
    "connect-history-api-fallback": "^1.3.0",
    "react": "15.2.1",
    "react-bootstrap": "0.29.5",
    "react-dom": "15.2.1",
    "react-router": "2.5.1"
  },
  "devDependencies": {
    "assert": "1.4.1",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "body-parser": "1.15.2",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "3.0.1",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "eslint": "3.0.1",
    "eslint-config-google": "0.6.0",
    "eslint-plugin-react": "5.1.1",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "mocha": "3.2.0",
    "morgan": "1.7.0",
    "rimraf": "2.4.3",
    "style-loader": "0.13.1",
    "typescript": "2.1.5",
    "wdio-junit-reporter": "0.2.0",
    "wdio-mocha-framework": "0.5.7",
    "webdriverio": "4.5.0",
    "webpack": "1.12.14"
  }
}
