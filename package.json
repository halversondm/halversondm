{
  "name": "halversondm",
  "version": "1.0.0",
  "description": "The personal website of Dan Halverson - @halversondm",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/halversondm/halversondm.git"
  },
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "prestart": "npm run clean && cp -R app/runtime/* dist && cd dist && npm install",
    "start": "cross-env NODE_ENV=development concurrently \"npm run webpack -- --watch\" \"node dist/server.js\"",
    "tslint": "tslint -c tslint.json --fix 'app/**/*.{ts,tsx}'",
    "test": "jest",
    "prewebpack": "npm run tslint && npm run test",
    "webpack": "webpack --config ./webpack.config.js --profile",
    "tsc": "tsc -w",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production npm run webpack",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "keywords": [
    "react"
  ],
  "author": "Dan Halverson <daniel.m.halverson@gmail.com> (http://halversondm.com/)",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.35.0",
    "@types/react": "17.0.6",
    "@types/react-dom": "*",
    "@types/react-router": "*",
    "connect-history-api-fallback": "*",
    "font-awesome": "*",
    "react": "*",
    "react-bootstrap": "*",
    "react-dom": "*",
    "react-router": "*",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "*"
  },
  "devDependencies": {
    "@types/jest": "*",
    "assert": "*",
    "awesome-typescript-loader": "*",
    "body-parser": "*",
    "concurrently": "*",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "*",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "express": "*",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "*",
    "mini-css-extract-plugin": "^1.6.0",
    "mocha": "*",
    "morgan": "*",
    "react-test-renderer": "*",
    "rimraf": "*",
    "style-loader": "*",
    "ts-jest": "*",
    "tslint": "*",
    "tslint-react": "*",
    "typescript": "*",
    "wdio-junit-reporter": "*",
    "webdriverio": "*",
    "webpack": "*",
    "webpack-cli": "*"
  }
}
