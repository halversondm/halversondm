{
  "name": "halversondm",
  "version": "1.0.0",
  "description": "The personal website of Dan Halverson - @halversondm",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/halversondm/halversondm.git"
  },
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "start": "webpack serve --config webpack.dev.js",
    "test": "vitest --config vitest.config.js",
    "test:coverage": "vitest --config vitest.config.js --coverage",
    "test:run": "vitest run --config vitest.config.js",
    "eslint": "eslint --fix ./app/",
    "prettier": "prettier . --write",
    "tsc": "tsc -w",
    "prebuild": "npm run clean && npm run test:run",
    "build": "webpack --config webpack.prod.js",
    "prepare": "husky install"
  },
  "keywords": [
    "react"
  ],
  "author": "Dan Halverson <daniel.m.halverson@gmail.com> (https://halversondm.com/)",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "*",
    "@aws-sdk/client-secrets-manager": "*",
    "@fortawesome/fontawesome-free": "*",
    "@fortawesome/fontawesome-svg-core": "*",
    "@fortawesome/free-brands-svg-icons": "*",
    "@fortawesome/free-regular-svg-icons": "*",
    "@fortawesome/free-solid-svg-icons": "*",
    "@fortawesome/react-fontawesome": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/react-router": "*",
    "@types/react-router-bootstrap": "*",
    "connect-history-api-fallback": "*",
    "font-awesome": "*",
    "react": "*",
    "react-bootstrap": "*",
    "react-dom": "*",
    "react-router": "*",
    "react-router-bootstrap": "*",
    "react-router-dom": "*"
  },
  "devDependencies": {
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "@types/jest": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@vitest/coverage-v8": "*",
    "assert": "*",
    "body-parser": "*",
    "copy-webpack-plugin": "*",
    "css-loader": "*",
    "css-minimizer-webpack-plugin": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-config-standard-with-typescript": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-n": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-react": "*",
    "file-loader": "*",
    "global-jsdom": "*",
    "happy-dom": "*",
    "html-webpack-plugin": "*",
    "husky": "*",
    "jsdom": "*",
    "lint-staged": "*",
    "mini-css-extract-plugin": "*",
    "msw": "*",
    "prettier": "*",
    "rimraf": "*",
    "style-loader": "*",
    "ts-loader": "*",
    "typescript": "*",
    "vitest": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*",
    "webpack-merge": "*"
  },
  "lint-staged": {
    "*.tsx": "eslint --fix ./app/",
    "*.{js,css,md}": "prettier --write"
  }
}
