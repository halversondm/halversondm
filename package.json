{
  "name": "halversondm",
  "version": "1.0.0",
  "description": "The personal website of Dan Halverson - @halversondm",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/halversondm/halversondm.git"
  },
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "start": "webpack serve --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --fix ./app/",
    "tsc": "tsc -w",
    "prebuild": "npm run clean && npm run eslint && npm run test",
    "build": "webpack --config webpack.prod.js",
    "prepare": "husky install"
  },
  "keywords": [
    "react"
  ],
  "author": "Dan Halverson <daniel.m.halverson@gmail.com> (https://halversondm.com/)",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "*",
    "@aws-sdk/client-secrets-manager": "*",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/react-router": "*",
    "connect-history-api-fallback": "*",
    "font-awesome": "*",
    "react": "*",
    "react-bootstrap": "*",
    "react-dom": "*",
    "react-router": "*",
    "react-router-bootstrap": "*",
    "react-router-dom": "*"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "assert": "*",
    "body-parser": "*",
    "concurrently": "*",
    "copy-webpack-plugin": "*",
    "cross-env": "*",
    "css-loader": "*",
    "css-minimizer-webpack-plugin": "*",
    "eslint": "^8.52.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "express": "*",
    "file-loader": "*",
    "html-webpack-plugin": "*",
    "husky": "^8.0.0",
    "jest": "*",
    "mini-css-extract-plugin": "*",
    "mocha": "*",
    "morgan": "*",
    "react-test-renderer": "*",
    "rimraf": "*",
    "style-loader": "*",
    "ts-jest": "*",
    "ts-loader": "*",
    "typescript": "^5.2.2",
    "wdio-junit-reporter": "*",
    "webdriverio": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
